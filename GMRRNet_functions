  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "b503eccb",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-14T15:24:39.642307Z",
     "iopub.status.busy": "2024-08-14T15:24:39.642000Z",
     "iopub.status.idle": "2024-08-14T15:24:39.653176Z",
     "shell.execute_reply": "2024-08-14T15:24:39.652282Z"
