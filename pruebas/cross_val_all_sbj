   "execution_count": 3,
   "id": "4163b539",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-12T14:23:20.172361Z",
     "iopub.status.busy": "2024-08-12T14:23:20.171802Z",
     "iopub.status.idle": "2024-08-12T14:23:20.204963Z",
     "shell.execute_reply": "2024-08-12T14:23:20.204155Z"
    },
    "papermill": {
     "duration": 0.049355,
     "end_time": "2024-08-12T14:23:20.206763",
     "exception": false,
     "start_time": "2024-08-12T14:23:20.157408",
